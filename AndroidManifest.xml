<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ColoradoTreeIdentifier">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.ColoradoTreeIdentifier.MainActivity"
            android:label="@string/app_name"

        android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ColoradoTreeIdentifier.NeedleCountActivity" />
        <activity android:name="com.ColoradoTreeIdentifier.FiveNeedle" />
        <activity android:name="com.ColoradoTreeIdentifier.BristleCone" />
        <activity android:name="com.ColoradoTreeIdentifier.LimberPine" />
        <activity android:name="com.ColoradoTreeIdentifier.PonderosaPine" />
        <activity android:name="com.ColoradoTreeIdentifier.TwoNeedle" />
        <activity android:name="com.ColoradoTreeIdentifier.PinyonPine" />
        <activity android:name="com.ColoradoTreeIdentifier.LodgepolePine" />
        <activity android:name="com.ColoradoTreeIdentifier.OneNeedle" />
        <activity android:name="com.ColoradoTreeIdentifier.Spruce" />
        <activity android:name="com.ColoradoTreeIdentifier.EngelmannSpruce" />
        <activity android:name="com.ColoradoTreeIdentifier.BlueSpruce" />
        <activity android:name="com.ColoradoTreeIdentifier.DouglasFir" />
        <activity android:name="com.ColoradoTreeIdentifier.Fir" />
        <activity android:name="com.ColoradoTreeIdentifier.WhiteFir" />
        <activity android:name="com.ColoradoTreeIdentifier.SubAlpineFir" />
    </application>

</manifest>